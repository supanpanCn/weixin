.swiper_wrapper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_wrapper image {
  width: 100%;
}
.nav {
  display: flex;
}
.nav navigator {
  padding: 20rpx;
  flex: 1;
}
.floor .tit {
  width: 100%;
  padding: 10rpx 0;
}
.floor .inner_goods {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 0 3px;
}
.floor .inner_goods navigator {
  width: 33.33%;
  float: left;
}
.floor .inner_goods navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor .inner_goods navigator:nth-child(2),
.floor .inner_goods navigator:nth-child(3) {
  border-bottom: 8rpx solid #fff;
}
.floor .inner_goods navigator image {
  width: 100%;
  height: 100%;
}
