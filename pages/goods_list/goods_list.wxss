.all navigator {
  display: flex;
  border-bottom: 1rpx solid #ccc;
}
.all navigator .l {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.all navigator .l image {
  width: 70%;
}
.all navigator .r {
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.all navigator .r .tit {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 2;
}
.all navigator .r .price {
  color: var(--themeColor);
  font-size: 32rpx;
}
